{
  "name": "xendit-test",
  "version": "1.0.0",
  "author": "Neil Palima",
  "main": "./dist/src/server.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/neilpalima/test.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=local && npm run start:dev || npm run start:prod",
    "start:prod": "node ./dist/src/server.js",
    "start:dev": "tsc-watch --onSuccess \"npm run start:prod\" ",
    "build": "./node_modules/typescript/bin/tsc -p ./tsconfig.json",
    "lint": "./node_modules/typescript/bin/tsc --noEmit && eslint \"{src,test}/*.{js,ts}\"",
    "lint:fix": "npm run lint -- --fix",
    "scripts:seeder": "node ./dist/scripts/seeder.js",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --exit -t 15000 -r ts-node/register test/*.ts",
    "test:coverage": "./node_modules/.bin/nyc npm test"
  },
  "dependencies": {
    "dotenv": "^6.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.16.4",
    "express-async-router": "^0.1.15",
    "faker": "^4.1.0",
    "if-env": "^1.0.4",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.29.0",
    "sequelize-typescript": "^1.0.0",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^3.0.5",
    "@types/dotenv": "^6.1.0",
    "@types/eslint": "^6.1.3",
    "@types/express": "^4.16.0",
    "@types/faker": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^12.11.7",
    "@types/sequelize": "^4.28.6",
    "@types/sinon": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "eslint": "^6.6.0",
    "mocha": "^5.2.0",
    "node-mocks-http": "^1.8.0",
    "nyc": "^14.1.1",
    "sinon": "^7.2.3",
    "ts-node": "^7.0.1",
    "tsc-watch": "^1.1.34"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "text",
      "lcov",
      "text-summary"
    ],
    "all": true,
    "check-coverage": true,
    "lines": 50,
    "statements": 50,
    "functions": 50,
    "branches": 50
  }
}
